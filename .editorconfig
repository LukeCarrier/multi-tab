root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{css,html,js,json,md,swift}]
indent_style = space
indent_size = 2

[*.xcworkspacedata]
indent_style = space
indent_size = 3

[*.storyboard]
indent_style = space
indent_size = 4

[*.{pbxproj,plist,entitlements}]
indent_style = tab
indent_size = 4
